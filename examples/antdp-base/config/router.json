[
  {
    "path": "/login",
    "component": "@/layouts/UserLayout"
  },
  {
    "path": "/",
    "component": "@/layouts/BasicLayout",
    "routes": [
      {
        "path": "/",
        "redirect": "/welcome"
      },
      {
        "path": "/welcome",
        "name": "首页",
        "icon": "welcome",
        "locale": "welcome",
        "component": "@/pages/Home/index"
      },
      {
        "path": "/dashboard",
        "name": "实例展示",
        "icon": "dashboard",
        "locale": "dashboard",
        "routes": [
          {
            "path": "/dashboard/demo",
            "name": "实例展示",
            "locale": "实例展示",
            "icon": "pie-chart",
            "component": "@/pages/Demo"
          },
          {
            "path": "/dashboard/workplace",
            "name": "工作台",
            "locale": "工作台",
            "icon": "pie-chart",
            "component": "@/pages/Dashboard"
          },
          {
            "path": "/dashboard/notfund",
            "name": "测试不存在页面",
            "icon": "radar-chart"
          }
        ]
      },
      {
        "path": "/EditTable",
        "name": "编辑表格",
        "icon": "file-protect",
        "component": "@/pages/EditTable"
      },
      {
        "path": "/404",
        "name": "404",
        "hideInMenu": true,
        "icon": "file-protect",
        "component": "@/pages/404"
      },
      {
        "path": "/403",
        "name": "403",
        "hideInMenu": true,
        "icon": "file-protect",
        "component": "@/pages/403"
      }
    ]
  }
