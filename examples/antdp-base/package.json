{
  "name": "@example/antdp-base",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@antdp/authorized": "^1.2.0",
    "@antdp/basic-layouts": "^1.2.0",
    "@antdp/page-loading": "^1.2.0",
    "@antdp/user-login": "^1.2.0",
    "antd": "4.2.0",
    "umi": "3.1.2"
  },
  "devDependencies": {
    "@antdp/config": "^1.2.0",
    "@antdp/dependencies": "^1.2.0",
    "@umijs/plugin-dva": "0.6.5",
    "@umijs/plugin-request": "2.3.2",
    "@umijs/test": "3.1.3",
    "lint-staged": "10.2.2",
    "prettier": "2.0.5"
  }
}
