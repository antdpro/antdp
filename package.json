{
  "name": "antdp-project",
  "private": true,
  "description": "Ant Design Project",
  "author": "jaywcjlove",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "website:install": "lerna bootstrap --scope @example/website --hoist",
    "start:antdp-base": "lerna exec --scope @example/antdp-base -- npm run start",
    "start:website": "lerna exec --scope @example/website -- npm run start",
    "start:basic": "lerna exec --scope @example/basic npm run start",
    "build:antdp-base": "lerna exec --scope @example/antdp-base -- npm run build",
    "build:website": "lerna exec --scope @example/website -- npm run build",
    "build": "lerna exec --scope @antdp/* --ignore @antdp/dependencies -- tsbb build",
    "umiinstall": "lerna exec --scope @example/antdp-base --scope @example/basic -- npm run umiinstall",
    "⬇️⬇️⬇️⬇️⬇️ install ⬇️⬇️⬇️⬇️⬇️": "▼▼▼▼▼ install::package ▼▼▼▼▼",
    "bootstrap": "lerna bootstrap",
    "hoist": "lerna bootstrap --hoist",
    "install:pkg": "lerna bootstrap --scope @antdp/* --hoist",
    "install:example": "lerna bootstrap --scope @example/* --hoist",
    "⬆️⬆️⬆️⬆️⬆️ install ⬆️⬆️⬆️⬆️⬆️": "▲▲▲▲▲ install::package ▲▲▲▲▲",
    "⬇️⬇️⬇️⬇️⬇️ watch::package ⬇️⬇️⬇️⬇️⬇️": "▼▼▼▼▼ watch::package ▼▼▼▼▼",
    "watch": "lerna exec --scope @antdp/* --ignore @antdp/dependencies -- tsbb watch",
    "watch:antdp-edit-table": "lerna exec  --scope @antdp/edit-table -- tsbb watch",
    "watch:antdp-fuzzy-query": "lerna exec  --scope @antdp/fuzzy-query -- tsbb watch",
    "watch:antdp-ui": "lerna exec  --scope @antdp/antdp-ui -- tsbb watch",
    "watch:authorized": "lerna exec  --scope @antdp/authorized -- tsbb watch",
    "watch:basic-layouts": "lerna exec  --scope @antdp/basic-layouts -- tsbb watch",
    "watch:config": "lerna exec  --scope @antdp/config -- tsbb watch",
    "watch:document-title": "lerna exec  --scope @antdp/document-title -- tsbb watch",
    "watch:fullscreen": "lerna exec --scope @antdp/fullscreen -- tsbb watch --entry src/index.js",
    "watch:icons": "lerna exec  --scope @antdp/icons -- tsbb watch",
    "watch:layout-tabs": "lerna exec  --scope @antdp/layout-tabs -- tsbb watch",
    "watch:page-loading": "lerna exec  --scope @antdp/page-loading -- tsbb watch",
    "watch:user-login": "lerna exec  --scope @antdp/user-login -- tsbb watch",
    "watch:hooks": "lerna exec  --scope @antdp/hooks -- tsbb watch",
    "⬆️⬆️⬆️⬆️⬆️ watch::package ⬆️⬆️⬆️⬆️⬆️": "▲▲▲▲▲ watch::package ▲▲▲▲▲",
    "version": "lerna version --exact --force-publish --no-push --no-git-tag-version",
    "publish": "lerna publish from-package",
    "start": "lerna exec --scope @example/antdp-base npm run start",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "clean": "lerna clean  --yes && npm run remove && rm -rf ./package-lock.json && rm -rf node_modules && npm run remove:c ",
    "remove": " lerna exec --scope @antdp/* --scope @example/*  -- rm -rf package-lock.json",
    "remove:c": "rm -rf package-lock.json &&  npm run remove:yarn && npm run remove:lib && npm run remove:esm",
    "remove:yarn": "lerna exec --scope @antdp/* --scope @example/*  -- rm -rf yarn.lock",
    "remove:lib": "lerna exec --scope @antdp/*  -- rm -rf ./lib",
    "remove:esm": "lerna exec --scope @antdp/*  -- rm -rf ./esm",
    "tsbb": "tsbb",
    "lerna": "lerna"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ]
  },
  "workspaces": {
    "packages": [
      "examples/*",
      "packages/*"
    ]
  },
  "overrides": {
    "react-dom": "^18.2.0",
    "react": "^18.2.0"
  },
  "resolutions": {
    "react-dom": "^18.2.0",
    "react": "^18.2.0"
  },
  "dependencies": {
    "compile-less-cli": "1.8.13",
    "husky": "8.0.3",
    "jest-environment-jsdom": "29.5.0",
    "lerna": "6.6.1",
    "lint-staged": "~13.0.4",
    "prettier": "2.8.1",
    "tsbb": "^3.7.7"
  }
}